# Python 3

class Solution:
    def primeSubOperation(self, nums: List[int]) -> bool:
        def is_prime(num):
            if num <= 1:
                return False
            for i in range(2, int(num ** 0.5) + 1):
                if num % i == 0:
                    return False
            return True
        max_num = max(nums)
        primes = [i for i in range(2, max_num) if is_prime(i)]
        x = 0
        for i in range(len(nums)):
            for prime in reversed(primes):
                if nums[i] - prime > x:
                    nums[i] -= prime
                    break
            if nums[i] <= x:
                return False
            x = nums[i]
        return True
