# Python 3

class Solution:
    def findDuplicates(self, nums: List[int]) -> List[int]:
        count_dict = {}
        result = []
        for num in nums:
            count_dict[num] = count_dict.get(num,0) + 1
        for num, count in count_dict.items():
            if count > 1:
                result.append(num)

        return(result)
