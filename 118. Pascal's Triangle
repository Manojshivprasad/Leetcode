# Python 3

class Solution:
    def generate(self, numRows: int) -> List[List[int]]:
        result = [[1],[1,1]]
        l1 = [1,1]
        for j in range(numRows-2):
            l2 = []
            for i in range(1,len(l1)):
                val = l1[i-1] + l1[i]
                l2.append(val)
            l2.insert(0,1)
            l2.append(1)
            result.append(l2)
            l1 = l2
        return(result[:numRows])
