# Python 3

class Solution:
    def getMaximumXor(self, nums: List[int], maximumBit: int) -> List[int]:
        result = []
        m = 0
        max_val = (1 << maximumBit) - 1
        for i in nums:
            m ^= i
            result.append(m ^ max_val)
        result.reverse()
        return(result)
